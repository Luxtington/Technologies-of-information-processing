        List<Point2D> pts = new ArrayList<>();
        pts.add(new Point2D(1,2));
        pts.add(new Point2D(4,5));
        pts.add(new Point2D(7,8));
        pts.add(new Point2D(1,6));

        //pts.sort(null); // comparable
        pts.sort(new Point2DCompatator()); // comparator
        System.out.println(pts);